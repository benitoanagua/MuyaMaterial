{
  "name": "muya-material",
  "displayName": "Muya Material",
  "description": "A modern Visual Studio Code theme inspired by Material Design 3",
  "version": "0.1.1",
  "publisher": "benitoanagua",
  "type": "module",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "assets/icon.jpg",
  "contributes": {
    "themes": [
      {
        "label": "Muya Material Black",
        "uiTheme": "vs-dark",
        "path": "./themes/material-black.json"
      },
      {
        "label": "Muya Material Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/material-dark.json"
      },
      {
        "label": "Muya Material Light",
        "uiTheme": "vs",
        "path": "./themes/material-light.json"
      },
      {
        "label": "Muya Material Soft",
        "uiTheme": "vs",
        "path": "./themes/material-soft.json"
      }
    ]
  },
  "scripts": {
    "build": "node src/index.js",
    "test": "vitest",
    "dev": "npm run build && npm test",
    "package": "vsce package",
    "watch": "nodemon --watch src --ext js,json --exec \"npm run build\"",
    "watch:test": "nodemon --watch src --ext js,json --exec \"npm run dev\""
  },
  "keywords": [
    "theme",
    "vscode-theme",
    "material-design",
    "color-theme"
  ],
  "author": "Benito Anagua",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benitoanagua/MuyaMaterial"
  },
  "bugs": {
    "url": "https://github.com/benitoanagua/MuyaMaterial/issues"
  },
  "homepage": "https://github.com/benitoanagua/MuyaMaterial#readme",
  "devDependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "chalk": "^5.6.2",
    "nodemon": "^3.1.10",
    "vitest": "^3.2.4",
    "vsce": "^2.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
